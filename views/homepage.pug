script var results = !{JSON.stringify(results)}
html
   head
      include googleAnalytics/analyticsScript.html
      title Library Programs
      style
        include css/style.css
   body
     div.header
       div.icon
         a(href="/") 
           img(src = "https://cdn.glitch.com/d7b2812e-18db-4d98-b36a-64ba8c765442%2FSuffolkLibraryPrograms_logo.png?v=1576465948961", alt = "Logo for Suffolk Library Programs", height = 86, width = 167)
       div.login
         a(href="/login") Librarian Tools
         br
         br
         div.showFilter Show filters
     div#filtersContainer
     div.bottom
       each n in results
         div.event
           div.progDate= n.day + ", " + (n.date.getMonth()+1) + "/" + n.date.getDate() + "/" + n.date.getFullYear() + " at " + n.time
           div.progName= n.programName
           div.greyRowContainer
             div.audience= n.audience
             div.delim  - 
             div.library= n.library
           div.progDesc= n.description
           div.programCat= n.programCat
           div.blankSpace 
           div.progLink
            a(href=n.regUrl target="_blank")= n.linkType 
include scripts/home.js
